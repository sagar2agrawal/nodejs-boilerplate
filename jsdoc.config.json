{
  "plugins": [
    "plugins/markdown", 
    "plugins/summarize"
  ],
  "source": {
    "include": [ "src/" ],
    "exclude": [ "node_modules" ]
  },
  "sourceType": "module",
    "tags": {
        "allowUnknownTags": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true
    }
}